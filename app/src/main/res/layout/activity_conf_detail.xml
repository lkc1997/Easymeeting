<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.lkc97.easymeeting.ui.common.ConfDetailActivity">
    <Button
        android:id="@+id/conf_participate_detail_activity_btn"
        android:onClick="confParticipate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="参加会议"/>

    <Button
        android:id="@+id/conf_quit_detail_activity_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="confQuit"
        android:text="退出会议" />
</LinearLayout>
